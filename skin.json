{
	"name": "Purple",
	"version": "1",
	"author": [
		"James Hare"
	],
	"url": "https://github.com/harej/purple",
	"descriptionmsg": "purple-desc",
	"license-name": "BSD-2-Clause",
	"type": "skin",
	"ValidSkinNames": {
		"purple": "Purple"
	},
	"MessagesDirs": {
